                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/Admin/NotesUnderReview">Notes Under Review</a>
                                    <a class="dropdown-item" href="#">Published Notes</a>
                                    <a class="dropdown-item" href="#">Downloaed Notes</a>
                                    <a class="dropdown-item" href="#">Rejected Notes</a>
                                </div>

                            </div>
                        </li>

                        <!-- Memebers -->
                        <li class="nav-item">
                            <a class="nav-link" href="#"><span>Members</span></a>
                        </li>

                        <!--Reports -->
                        <li class="nav-item">

                            <div class="dropdown">

                                <a class="nav-link" href="#" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><span>Reports</span></a>

                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">Spam Reports</a>
